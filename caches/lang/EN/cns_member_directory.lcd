a:43:{s:29:"BLOCK_TRANS_NAME_main_members";s:14:"Member display";s:22:"BLOCK_main_members_USE";s:71:"Used within the member directory, and also for displaying custom views.";s:30:"BLOCK_main_members_DESCRIPTION";s:49:"Display members according to a series of filters.";s:43:"BLOCK_main_members_PARAM_display_mode_TITLE";s:12:"Display mode";s:47:"BLOCK_main_members_PARAM_must_have_avatar_TITLE";s:16:"Must have avatar";s:46:"BLOCK_main_members_PARAM_must_have_photo_TITLE";s:15:"Must have photo";s:43:"BLOCK_main_members_PARAM_include_form_TITLE";s:12:"Include form";s:37:"BLOCK_main_members_PARAM_select_TITLE";s:10:"Selectcode";s:37:"BLOCK_main_members_PARAM_filter_TITLE";s:10:"Filtercode";s:44:"BLOCK_main_members_PARAM_filters_row_a_TITLE";s:13:"Filters row A";s:44:"BLOCK_main_members_PARAM_filters_row_b_TITLE";s:13:"Filters row B";s:40:"BLOCK_main_members_PARAM_usergroup_TITLE";s:9:"Usergroup";s:34:"BLOCK_main_members_PARAM_max_TITLE";s:15:"Maximum to show";s:36:"BLOCK_main_members_PARAM_start_TITLE";s:14:"Start position";s:41:"BLOCK_main_members_PARAM_pagination_TITLE";s:10:"Pagination";s:35:"BLOCK_main_members_PARAM_sort_TITLE";s:7:"Sort by";s:45:"BLOCK_main_members_PARAM_parent_gallery_TITLE";s:14:"Parent gallery";s:38:"BLOCK_main_members_PARAM_per_row_TITLE";s:14:"Number per-row";s:35:"BLOCK_main_members_PARAM_guid_TITLE";s:4:"GUID";s:37:"BLOCK_main_members_PARAM_display_mode";s:111:"The display mode (must be either 'avatars' or 'photos' or 'media' or 'boxes' or 'listing'). Default: 'avatars'.";s:41:"BLOCK_main_members_PARAM_must_have_avatar";s:58:"Whether only members with avatars are shown. Default: '0'.";s:40:"BLOCK_main_members_PARAM_must_have_photo";s:57:"Whether only members with photos are shown. Default: '0'.";s:37:"BLOCK_main_members_PARAM_include_form";s:49:"Whether to include the filter form. Default: '1'.";s:31:"BLOCK_main_members_PARAM_select";s:55:"Selectcode specifying specific member IDs. Default: ''.";s:38:"BLOCK_main_members_PARAM_filters_row_a";s:267:"A comma-separated list of filter field mappings. This is automatically added to the defined Filtercode, and automatically used to put together a filtering interface. Blank means put together a default filter with the username, usergroup, and select CPFs. Default: ''.";s:38:"BLOCK_main_members_PARAM_filters_row_b";s:126:"A second row of filters. If you specify a second row of filters, a reset button will be included on the form too. Default: ''.";s:31:"BLOCK_main_members_PARAM_filter";s:116:"Advanced feature. Filtercode syntax for filtering results based on field comparisons. Default: no Filtercode filter.";s:34:"BLOCK_main_members_PARAM_usergroup";s:92:"A usergroup name or usergroup ID to limit to. May be multiple, comma-separated. Default: ''.";s:28:"BLOCK_main_members_PARAM_max";s:49:"Maximum number of members to show. Default: '30'.";s:30:"BLOCK_main_members_PARAM_start";s:70:"Advanced feature. The pagination position to start from. Default: '0'.";s:35:"BLOCK_main_members_PARAM_pagination";s:42:"Whether to paginate results. Default: '0'.";s:29:"BLOCK_main_members_PARAM_sort";s:372:"The sort order (must be either 'm_cache_num_posts ASC' or 'm_cache_num_posts DESC' or 'm_last_visit_time ASC' or 'm_last_visit_time DESC' or 'm_profile_views ASC' or 'm_profile_views DESC' or 'm_total_sessions ASC' or 'm_total_sessions DESC' or 'm_join_time ASC' or 'm_join_time DESC' or 'm_username ASC' or 'm_username DESC' or 'random ASC'). Default: 'm_join_time DESC'.";s:39:"BLOCK_main_members_PARAM_parent_gallery";s:153:"Advanced feature. The parent gallery that a member personal gallery will be searched at. This only is relevant for the 'media' display mode. Default: ''.";s:32:"BLOCK_main_members_PARAM_per_row";s:116:"The number of results to show per row, if applicable. If left blank it will fill based on fixed sizing. Default: ''.";s:29:"BLOCK_main_members_PARAM_guid";s:87:"Advanced feature. Relay a specific GUID through to all involved templates. Default: ''.";s:13:"PROFILE_VIEWS";s:13:"Profile views";s:15:"LOGIN_FREQUENCY";s:15:"Login frequency";s:27:"MEMBER_DIRECTORY_UNFILTERED";s:44:"We have the following members on {1}&hellip;";s:25:"MEMBER_DIRECTORY_FILTERED";s:51:"The following {1} members match your search&hellip;";s:38:"MEMBER_DIRECTORY_UNFILTERED_NO_RESULTS";s:22:"No members were found.";s:48:"MEMBER_DIRECTORY_UNFILTERED_NO_RESULTS_GALLERIES";s:31:"No member galleries were found.";s:36:"MEMBER_DIRECTORY_FILTERED_NO_RESULTS";s:41:"No results were returned for your search.";s:12:"RESET_FILTER";s:5:"Reset";}